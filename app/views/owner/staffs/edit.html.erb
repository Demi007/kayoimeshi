<h1>スタッフ編集</h1>
<%= form_with model: @staff, url: owner_restaurant_staff_path, local:true do |f| %>
  <h4>ニックネーム</h4>
  <%= f.text_field :nick_name %>
  <h4>自己紹介</h4>
  <%= f.text_area :introduction %>
  <%= f.submit '編集を保存' %>
<% end %>
  <%= link_to "削除", owner_restaurant_staff_path, method: :delete %>